  "description": "stateless password manager",
  "bin": {
    "viridium": "viridium.js"
  },
  "scripts": {
    "format": "npx prettier --write viridium.js"
  },
  "author": "Aria Buckles",
  "license": "MIT",
  "dependencies": {
